    "prepublishOnly": "npm run build"
  },
  "author": "Jonas Strassel <jonas@bdglabs.com>",
  "license": "MIT",
  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@bgd-labs/toolbox": "^0.0.42",
    "@biomejs/biome": "^2.1.3",
    "@types/node": "^24.2.0",
    "lefthook": "^1.12.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "tsup": {
    "entry": [
      "src/action.ts"
    ],
    "splitting": false,
    "sourcemap": false,
    "clean": true,
    "dts": true,
    "treeshake": true,
